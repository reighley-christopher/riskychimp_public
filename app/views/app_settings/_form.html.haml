= form_for @app_setting do |f|
  - if @app_setting.errors.any?
    #error_explanation
      %h2= "#{pluralize(@app_setting.errors.count, "error")} prohibited this app_setting from being saved:"
      %ul
        - @app_setting.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :value
    = f.text_field :value
  .actions
    = f.submit 'Save'
